	<queryString>
		<![CDATA[SELECT TRUNC(BV.NUMERO_CREDITO) AS NUMERO_CREDITO,
T.NUMERO_APROBACION AS NUMERO_APROBACION,
'Pagares' as FORMA_PAGO,
T.REFERENCIA1 AS CEDULA,
T.TOTAL_NEGOCIO AS VALOR_COMPRA,
(select VALOR from TPAGARE_TASA_INTERES WHERE CODIGO_AFILIADO=T.AFILIADO) AS INTERES,
T.FECHA AS FECHA,
( T.REFERENCIA2) as NOMBRE_CLIENTE,
(SELECT COUNT(ID_MALI) AS CUOTAS FROM TPAGARE_DETALLE_LIQUIDACION WHERE
ID_MALI=(SELECT ID_MALI FROM TPAGARE_MAESTRO_LIQUIDACION WHERE NUMERO_APROBACION=$P{NOAUTORIZACION})) AS NUMERO_CUOTAS,
BV.VALOR_TOTAL AS TOTAL_FINANCIAR,
(SELECT VALOR_CUOTA FROM TPAGARE_DETALLE_LIQUIDACION WHERE ID_MALI =T.ID_MALI AND NUMERO_CUOTA=(
SELECT MAX(NUMERO_CUOTA) FROM TPAGARE_DETALLE_LIQUIDACION WHERE ID_MALI =T.ID_MALI)) +11900 AS VALOR_CUOTA
FROM V_BASICOS_VOUCHER@OPEN_CARD BV,TPAGARE_MAESTRO_LIQUIDACION T
WHERE BV.NUMERO_APROBACION=T.NUMERO_APROBACION
AND T.NUMERO_APROBACION=$P{NOAUTORIZACION}]]>
	</queryString>
