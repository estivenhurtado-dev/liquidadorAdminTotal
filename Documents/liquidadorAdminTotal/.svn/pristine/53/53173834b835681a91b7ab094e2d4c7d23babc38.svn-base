	<queryString>
		<![CDATA[SELECT NUMERO_APROBACION, (TO_NUMBER(TOTAL_NEGOCIO)+TO_NUMBER((CALCULAR_SEGURO_CREDITOENLINEA($P{NOAUTORIZACION})))) AS TOTAL_NEGOCIO
,
(SELECT NUMERO_APROBACION FROM TPAGARE_MAESTRO_LIQUIDACION WHERE CREDITO_SEGURO=$P{NOAUTORIZACION}) as AUTOSEGURO,

TO_NUMBER(TOTAL_NEGOCIO) + DECODE(REFERENCIA2,null,0,TO_NUMBER(REFERENCIA2))+
TO_NUMBER((CALCULAR_SEGURO_CREDITOENLINEA($P{NOAUTORIZACION})))++TO_NUMBER(ROUND(CALCULAR_COMI_CREDITOENLINEA($P{NOAUTORIZACION})))
AS TOTAL_NEGOCIO2,
TOTAL_NEGOCIO AS  TOTALNEGOCIOB , T_PRODUCTOS_FINANCIEROS.DESCRIPCION AS AFILIADO,FECHA, REFERENCIA5,REFERENCIA6,
TO_NUMBER(REFERENCIA2)+TO_NUMBER(ROUND(CALCULAR_COMI_CREDITOENLINEA($P{NOAUTORIZACION}))) AS REFERENCIA2,
T_PRODUCTOS_FINANCIEROS.TASA_FINANCIAMIENTO,
(SELECT VALOR_CUOTA FROM TPAGARE_DETALLE_LIQUIDACION WHERE ID_MALI= (SELECT ID_MALI FROM TPAGARE_MAESTRO_LIQUIDACION WHERE NUMERO_APROBACION=$P{NOAUTORIZACION}) AND ROWNUM=1)
+
(SELECT VALOR_CUOTA FROM TPAGARE_DETALLE_LIQUIDACION WHERE ID_MALI= (SELECT ID_MALI FROM TPAGARE_MAESTRO_LIQUIDACION WHERE CREDITO_SEGURO=$P{NOAUTORIZACION}) AND ROWNUM=1) +11900
AS VALORCUOTA,
(SELECT COUNT(ID_MALI) AS CUOTAS FROM TPAGARE_DETALLE_LIQUIDACION WHERE
ID_MALI=(SELECT ID_MALI FROM TPAGARE_MAESTRO_LIQUIDACION WHERE NUMERO_APROBACION=$P{NOAUTORIZACION})) AS CUOTAS,
T_PRODUCTOS_FINANCIEROS.COMISION || ' %'  AS COMISION
FROM TPAGARE_MAESTRO_LIQUIDACION,T_PRODUCTOS_FINANCIEROS
WHERE
TPAGARE_MAESTRO_LIQUIDACION.AFILIADO=T_PRODUCTOS_FINANCIEROS.CODIGO
AND
NUMERO_APROBACION=$P{NOAUTORIZACION}]]>
	</queryString>
