	<parameter name="NUMERO_APROBACION" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
initcap(ET.NOMBRE_ENTIDAD_TERRITORIAL) AS CIUDAD,
     TRIM(CC.CODIGO_CLIENTE) AS CODIGO_CLIENTE,
     TRIM(trunc(CC.NUMERO_CREDITO)) AS NUMERO_CREDITO,
     TRIM(CC.NOMBRE1_CLIENTE)
     ||' '||TRIM(CC.NOMBRE2_CLIENTE)
     ||' '||TRIM(CC.APELLIDO1_CLIENTE)
     ||' '||TRIM(CC.APELLIDO2_CLIENTE)  AS NOMBRE1_CLIENTE,
     trunc(CC.CEDULA_CLIENTE) AS CEDULA_CLIENTE,
     trim(CC.DIREECION_CLIENTE) AS DIREECION_CLIENTE,
     CC.TELEFONO1_CLIENTE AS TELEFONO1_CLIENTE,
     TO_DATE(BV.FECHA_HORA_TRANSACCION,'DD/MM/YY') AS FECHA_AUTORIZACION,
     TO_CHAR(TO_DATE(BV.FECHA_HORA_TRANSACCION,'DD/MM/YY'),'DD') AS DIA_AUTORIZACION,
     TO_CHAR(TO_DATE(BV.FECHA_HORA_TRANSACCION,'DD/MM/YY'),'YYYY') AS ANIO_AUTORIZACION,
(
  CASE
  WHEN EXTRACT(MONTH  FROM TO_DATE(BV.FECHA_HORA_TRANSACCION,'DD/MM/YY') )=1 THEN 'Enero'
  WHEN EXTRACT(MONTH  FROM TO_DATE(BV.FECHA_HORA_TRANSACCION,'DD/MM/YY') )=2 THEN 'Febrero'
  WHEN EXTRACT(MONTH  FROM TO_DATE(BV.FECHA_HORA_TRANSACCION,'DD/MM/YY') )=3 THEN 'Marzo'
  WHEN EXTRACT(MONTH  FROM TO_DATE(BV.FECHA_HORA_TRANSACCION,'DD/MM/YY') )=4 THEN 'Abril'
  WHEN EXTRACT(MONTH  FROM TO_DATE(BV.FECHA_HORA_TRANSACCION,'DD/MM/YY') )=5 THEN 'Mayo'
  WHEN EXTRACT(MONTH  FROM TO_DATE(BV.FECHA_HORA_TRANSACCION,'DD/MM/YY') )=6 THEN 'Junio'
  WHEN EXTRACT(MONTH  FROM TO_DATE(BV.FECHA_HORA_TRANSACCION,'DD/MM/YY') )=7 THEN 'Julio'
  WHEN EXTRACT(MONTH  FROM TO_DATE(BV.FECHA_HORA_TRANSACCION,'DD/MM/YY') )=8 THEN 'Agosto'
  WHEN EXTRACT(MONTH  FROM TO_DATE(BV.FECHA_HORA_TRANSACCION,'DD/MM/YY') )=9 THEN 'Septiembre'
  WHEN EXTRACT(MONTH  FROM TO_DATE(BV.FECHA_HORA_TRANSACCION,'DD/MM/YY') )=10 THEN 'Octubre'
  WHEN EXTRACT(MONTH  FROM TO_DATE(BV.FECHA_HORA_TRANSACCION,'DD/MM/YY') )=11 THEN 'Noviembre'
  WHEN EXTRACT(MONTH  FROM TO_DATE(BV.FECHA_HORA_TRANSACCION,'DD/MM/YY') )=12 THEN 'Diciembre'

  END
) as MES_AUTORIZACION,
(
  CASE
  WHEN EXTRACT(MONTH  FROM SYSDATE )=1 THEN 'Enero'
  WHEN EXTRACT(MONTH  FROM SYSDATE )=2 THEN 'Febrero'
  WHEN EXTRACT(MONTH  FROM SYSDATE )=3 THEN 'Marzo'
  WHEN EXTRACT(MONTH  FROM SYSDATE )=4 THEN 'Abril'
  WHEN EXTRACT(MONTH  FROM SYSDATE )=5 THEN 'Mayo'
  WHEN EXTRACT(MONTH  FROM SYSDATE )=6 THEN 'Junio'
  WHEN EXTRACT(MONTH  FROM SYSDATE )=7 THEN 'Julio'
  WHEN EXTRACT(MONTH  FROM SYSDATE )=8 THEN 'Agosto'
  WHEN EXTRACT(MONTH  FROM SYSDATE )=9 THEN 'Septiembre'
  WHEN EXTRACT(MONTH  FROM SYSDATE )=10 THEN 'Octubre'
  WHEN EXTRACT(MONTH  FROM SYSDATE )=11 THEN 'Noviembre'
  WHEN EXTRACT(MONTH  FROM SYSDATE )=12 THEN 'Diciembre'

  END
) as MES_ACTUAL,
TO_CHAR(TO_DATE(SYSDATE,'DD/MM/YY'),'DD') AS DIA_ACTIAL,
TO_CHAR(TO_DATE(SYSDATE,'DD/MM/YY'),'YYYY') AS ANIO_ACTUAL
FROM

NU_ENTIDADES_TERRITORIALES ET,
NU_DIRECCIONES D,
V_BASICOS_CLIENTE_CODEUDOR CC ,
V_BASICOS_VOUCHER BV
WHERE CC.CEDULA_CLIENTE=BV.CEDULA_CLIENTE
AND CC.CODIGO_CLIENTE=D.CODIGO_CLIENTE
AND D.PAIS=ET.PAIS
AND D.MUNICIPIO=ET.MUNICIPIO
AND D.DEPARTAMENTO=ET.DEPARTAMENTO
AND CORRESPONDENCIA_CLIENTE='S'
AND D.MUNICIPIO=ET.MUNICIPIO
AND CC.NUMERO_CREDITO IS NOT NULL
AND CC.NUMERO_CREDITO=BV.NUMERO_CREDITO
AND D.DEPARTAMENTO=ET.DEPARTAMENTO
AND trunc(BV.NUMERO_APROBACION)=$P{NUMERO_APROBACION}]]>
	</queryString>
