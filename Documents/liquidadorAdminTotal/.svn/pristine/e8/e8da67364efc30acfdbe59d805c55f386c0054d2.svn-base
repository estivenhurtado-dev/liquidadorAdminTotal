<!DOCTYPE html>
<html xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.org/ui">
<h:head>
	<meta content='text/html; charset=UTF-8' http-equiv="Content-Type" />
	<meta http-equiv="Pragma" content="no-cache" />
	<meta http-equiv="Expires" content="0" />
	<h:outputStylesheet library="css" name="default.css" />
	<h:outputScript library="js" name="validaciones.js" />

	<style>
.ui-blockui {
	background-color: black;
}

.ui-blockui-content {
	background-color: black;
}

.ui-widget-overlay {
	background: black;
	opacity: .8;
}

.ui-widget-overlay {
	background-color: black;
}
</style>


<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-73273572-1', 'auto');
  ga('send', 'pageview');

</script>




</h:head>
<h:body onload="#{MBLiquidacionPre.pintarParametrosRequest()}">
	<h:form id="form1">


		<div align="center">

			<p:growl id="messages1" sticky="true" />

			<p:panel style="width: 900px;" header="Generación Pagaré-Voucher"
				id="formularioPagareVoucher">
				<h:panelGrid id="forpagare" columns="2" cellpadding="2">
					<h:outputText rendered="#{MBLiquidacionPre.codigoReportes}" style="background-color: none; "
						value="Codigo del Afiliado: " />
					<p:inputText rendered="#{MBLiquidacionPre.codigoReportes}" style="width: 200px;" id="dato8" maxlength="20"
						onkeyup="javascript:this.value = stripNonNumeric(this.value);"
						value="#{MBLiquidacionPre.reporteCodigoAfiliado}">
					</p:inputText>
		
		
		
		
		
		
					<h:outputText style="background-color: none; "
						value="Cedula Cliente: " />
					<p:inputText style="width: 200px;" id="dato9" maxlength="20"
						onkeyup="javascript:this.value = stripNonNumeric(this.value);"
						value="#{MBLiquidacionPre.reporteCedulaCliente}">
					</p:inputText>
					<h:outputText style="background-color: none; "
						value="Número de Aprobación: " />
					<p:inputText style="width: 200px;" id="dato10" maxlength="20"
						onkeyup="javascript:this.value = stripNonNumeric(this.value);"
						value="#{MBLiquidacionPre.reporteNumeroAprobacion}">
					</p:inputText>

				</h:panelGrid>

				<h:panelGrid id="forpagaref" columns="2" cellpadding="2">


					<p:commandButton value="Reportes" style="width: 180px;"
						disabled="#{MBLiquidacionPre.genRepo}" styleClass="btn btn-green"
						id="reportes"
						update="messages1,fordescargareportes,forpagaref2,forpagaref"
						action="#{MBLiquidacionPre.generarReportes}">

					</p:commandButton>
					<p:blockUI block="reportes" trigger="reportes">  
                                Espere...<br />
					</p:blockUI>
				</h:panelGrid>



				<h:panelGrid id="fordescargareportes" columns="3" cellpadding="2">
					<p:commandButton rendered="#{MBLiquidacionPre.rcarta}"
						value="Carta Cesión" style="width: 180px;"
						actionListener="#{MBLiquidacionPre.generarCartaCesion}"
						ajax="false" styleClass="btn btn-green" id="pdf3">
					</p:commandButton>
					<p:commandButton rendered="#{MBLiquidacionPre.rpagare}"
						value="Pagaré" style="width: 180px;"
						actionListener="#{MBLiquidacionPre.generarPagare}" ajax="false"
						styleClass="btn btn-green" id="pdf4">
					</p:commandButton>
					<p:commandButton rendered="#{MBLiquidacionPre.rvoucher}"
						value="Voucher" style="width: 180px;"
						actionListener="#{MBLiquidacionPre.generarVoucher}" ajax="false"
						styleClass="btn btn-green" id="pdf5">
					</p:commandButton>
					<p:commandButton rendered="#{MBLiquidacionPre.rliquidacion}" value="Liquidación"
						style="width: 180px;"
						actionListener="#{MBLiquidacionPre.generarReportePreliquidacion}"
						ajax="false" styleClass="btn btn-green" id="pdf6">
					</p:commandButton>
				</h:panelGrid>


				<h:panelGrid id="forpagaref2" columns="2" cellpadding="2">
					<p:commandButton value="Cancelar" style="width: 180px;"
						rendered="#{MBLiquidacionPre.rgenRepo}" styleClass="btn btn-green"
						id="reportesrr"
						update="messages1,fordescargareportes,forpagaref2,forpagaref"
						action="#{MBLiquidacionPre.regenerarRepor}">

					</p:commandButton>
				</h:panelGrid>

			</p:panel>








		</div>
	</h:form>
</h:body>
</html>

